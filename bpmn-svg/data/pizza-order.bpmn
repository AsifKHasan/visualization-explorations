/*
1. to parse the bpmn only, run
    python bpmn-parser.py < ../data/pizza-order.bpmn > ../out/pizza-order.json
    it will generate json output at ../out/pizza-order.json

2. to parse the bpmn and generate the svg, run
    python bpmn-parser.py < ../data/pizza-order.bpmn | tee ../out/pizza-order.json | python bpmn-to-svg.py
    it will generate svg output at ../out/pizza-order.svg

3. to generate svg from an existing parsed json (generated by bpmn-parser.py), run
    python bpmn-to-svg.py < ../out/pizza-order.json
    it will generate svg output at ../out/pizza-order.svg
*/
graph pizza_order {
    label = "Pizza Order Process Model"

    lane customer {
        label = "Customer"
        pool pizza_customer {
            label = "Pizza Customer"

            start hungry [label="Hungry";]
            task A1b_id [label="Select Pizza";]
            task A1c_id [label="Order Pizza";]
            task A1c_id [label="A1c_label";]
            task A1c_id [label="A1c_label";]
            task A1c_id [label="A1c_label";]
            task A1c_id [label="A1c_label";]
            task A1c_id [label="A1c_label";]
            task A1c_id [label="A1c_label";]
            end hunger_satisfied [label="Hunger Satisfied";]

            A1a_id ..> A1b_id
            A1b_id ..> A1c_id
            A1c_id ..> A1d_id
        }
    }
    lane pizza_store {
        label = "Pizza Store"
        pool receptionist {
            label = "Receptionist"

            B1a_type B1a_id [label="B1a_label";]
            B1b_type B1b_id [label="B1b_label";]
            B1c_type B1c_id [label="B1c_label";]
            B1d_type B1d_id [label="B1d_label";]

            B1a_id ..> B1b_id
            B1b_id ..> B1c_id
            B1c_id ..> B1d_id
        }
        pool cook {
            label = "Pizza Cook"

            B2a_type B2a_id [label="B2a_label";]
            B2b_type B2b_id [label="B2b_label";]
            B2c_type B2c_id [label="B2c_label";]

            B2a_id ..> B2b_id
            B2b_id ..> B2c_id
        }
        pool delivery_person {
            label = "Delivery Person"

            A2a_type A2a_id [label="A2a_label";]
            A2b_type A2b_id [label="A2b_label";]
            A2c_type A2c_id [label="A2c_label";]

            A2a_id ..> A2b_id
            A2b_id ..> A2c_id
        }
    }
}
