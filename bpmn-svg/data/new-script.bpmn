graph bpmn_id {
    label = "BPMN Title"
    _ []

    lane lane_a {
        label = "Lane A Label"
        _ []

        pool pool_a1 {
            label = "Pool A1 Label"
            _ []

            start                           start1                              [label="start";]
            task                            task_a1_01                          [label="Task A1-01";]

            start1                          --> task_a1_01                      [label="";]
        }

        pool pool_a2 {
            label = "Pool A2 Label"
            _ []

            task                            task_a2_01                          [label="Task A2-01";]
        }

        pool pool_a3 {
            label = "Pool A3 Label"
            _ []

            task                            task_a3_01                          [label="Task A3-01";]
            end                             end1                                [label="end";]

            task_a3_01                      --> end1                            [label="";]
        }

        task_a1_01                      --> task_a2_01                      [label="";]
        task_a2_01                      --> task_a3_01                      [label="";]
    }
}