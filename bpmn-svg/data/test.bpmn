graph pizza_orders {
    label = "Pizza Order Process Modeling"
    _ [hide_labels="false";]

    lane customer {
        label = "Customer"
        _ []

        pool pizza_customer {
            label = "Pizza Customer"
            _ []

            start                      a_e1                    [label="A e1";]
            task 		                   a_t1		                 [label="A t1";]
            exclusive                  a_x1                    [label="A x1";]
            task 		                   a_t2		                 [label="A t2";]
            exclusive                  a_x2                    [label="A x2";]
            task 		                   a_t3		                 [label="A t3";]
            end                        a_e2                    [label="A e2";]

            task 		                   b_t1		                 [label="B t1";]
            exclusive                  b_x1                    [label="B x1";]
            task 		                   b_t2		                 [label="B t2";]

            task 		                   c_t1		                 [label="C t1";]
            exclusive                  c_x1                    [label="C x1";]
            task 		                   c_t2		                 [label="C t2";]

            task 		                   d_t1		                 [label="D t1";]
            task 		                   d_t2		                 [label="D t2";]

            a_e1                       --> a_t1                [label="";]
            a_t1                       --> a_x1                [label="";]
            a_x1                       --> a_t2                [label="";]
            a_t2                       --> a_x2                [label="";]
            a_x2                       --> a_t3                [label="";]
            a_t3                       --> a_e2                [label="";]

            a_x1                       --> b_t1                [label="";]
            b_t1                       --> b_x1                [label="";]
            b_x1                       --> b_t2                [label="";]
            b_t2                       --> a_e2                [label="";]

            b_x1                       --> c_t1                [label="";]
            c_t1                       --> c_x1                [label="";]
            c_x1                       --> c_t2                [label="";]
            c_t2                       --> a_e2                [label="";]

            c_x1                       --> d_t1                [label="";]
            d_t1                       --> d_t2                [label="";]
            d_t2                       --> a_e2                [label="";]

            #d_t1                       --> a_t2                [label="";]
            #d_t1                       --> b_t2                [label="";]
            d_t1                       --> c_t2                [label="";]

            #d_t2                       --> a_t2                [label="";]
            #d_t2                       --> b_t2                [label="";]
            #d_t2                       --> c_t2                [label="";]

        }
    }
}
